{
    "name": "thepublicgood/laravel-image-renderer",
    "description": "A simple solution for dealing with images in laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Warrick Bayman",
            "email": "me@warrickbayman.co.za"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "TPG\\ImageRenderer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPG\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TPG\\ImageRenderer\\ImageRendererServiceProvider"
            ],
            "aliases": {
                "ImageRenderer": "TPG\\ImageRenderer\\Facades\\ImageRenderer"
            }
        }
    }
}
